package util;

public class ConnectionTest {

	public static void main(String[] args) throws Exception {
		JdbcConnection jdbc1 = new JdbcConnection();
		System.out.println(jdbc1.con);
		
		// 1) sql문 작성
		String sql = "insert into testMember values(?,?)";
		// 2) psmt 객체 생성
		jdbc1.psmt = jdbc1.con.prepareStatement(sql);
		// 3) ? 채우기
		jdbc1.psmt.setString(1, "asdf");
		jdbc1.psmt.setString(2, "1234");
		// 4) 실행
		int res = jdbc1.psmt.executeUpdate();
		System.out.println("res : " + res);
	}
}
